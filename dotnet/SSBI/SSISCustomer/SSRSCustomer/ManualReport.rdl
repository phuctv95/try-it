  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StatesId">
            <Value>=Parameters!StatesId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        FactCustomer.CustomerCode, FactCustomer.CustomerName, FactCustomer.CustomerAmount, FactCustomer.SalesDate, DimSalesPerson.PersonName, DimStates.StatesName, DimProduct.ProductName, 
                         DimCountry.CountryName
FROM            FactCustomer INNER JOIN
                         DimStates ON FactCustomer.StatesId = DimStates.StatesId INNER JOIN
                         DimCountry ON FactCustomer.CountryId = DimCountry.CountryId INNER JOIN
                         DimProduct ON FactCustomer.ProductId = DimProduct.ProductId INNER JOIN
                         DimSalesPerson ON FactCustomer.SalesPersonId = DimSalesPerson.PersonId
WHERE        (FactCustomer.StatesId = @StatesId)</CommandText>
      </Query>
