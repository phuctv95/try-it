    <DataSet Name="DsCountry">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        CountryId, CountryName, IsNew
FROM            DimCountry
WHERE        (IsNew = 1)</CommandText>
      </Query>
